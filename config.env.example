# Enhanced MCP Server Configuration
# Copy this file to .env and customize the values

# MCP Server Configuration
MCP_SERVER_NAME=enhanced-mcp-server
MCP_LOG_LEVEL=INFO

# Agent System Configuration
MAX_CONCURRENT_AGENTS=5
AGENT_TIMEOUT=30000
ENABLE_COORDINATOR=true
ENABLE_SPECIALIZED_AGENTS=false
AGENT_AUTO_START=false

# Database Configuration (Qdrant Vector Database)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_api_key_here
ENABLE_VECTOR_DB=false

# LLM Integration Configuration
ENABLE_CURSOR_LLM=true
ENABLE_DOCKER_OLLAMA=false
OLLAMA_URL=http://localhost:11434
DEFAULT_MODEL=cursor-auto

# Development Configuration
DEBUG=false
LOG_TO_FILE=false
LOG_FILE_PATH=logs/mcp_server.log
